package com.codeprep.unary;

public class Increment_Decrement {
	public static void main(String[] args) {
		
//		Example 1.
		
//		int x = 5;		 //6       7
//		System.out.println(x++ + ++x);
//						 //5   +   7 --> 12
		
//		Example 2. 
		
		int i = 1;    //--> i=1
		int j = i++;  //--> j=1 & i=2
		
		System.out.println(i);  //2
		System.out.println(j);  //1
		
		
		
		
		
		
		
		
		
		
		
		
	}
}
