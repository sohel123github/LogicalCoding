package com.naveen.coding;

public class LargestAmongThreeNo {

	public static void main(String[] args) {
			
		int x = 200;
		int y = 100;
		int z = 300;
		
		if(x>y && x>z) {
			System.out.println("X is greater");
		}else if(y > z) {
			System.out.println("Y is greater");
		}else {
			System.out.println("Z is greater");
		}
		
	}

}
